	<property id="includeSubGroups" resourceName="includeSubGroups" dataType="bool" defaultValue="true" />
	<property id="pageSize" resourceName="pageSize" dataType="int" defaultValue="5">
		<propertyRule type="Telligent.Evolution.Controls.PropertyRules.MinMaxValueRule, Telligent.Evolution.Platform" minValue="1" maxValue="100" />
	</property>
</propertyGroup>
			]]></configuration>
