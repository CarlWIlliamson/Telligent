	<scriptedContentFragment name="${resource:Idea_List_Name}" version="10.1.8.10498" description="${resource:Idea_List_Description}" instanceIdentifier="5574af2a95e745b8b58751cc6e528466" theme="" isCacheable="true" varyCacheByUser="true" showHeaderByDefault="true" cssClass="idea-list">
		<contentScript><![CDATA[
#set ($showSidebarList = false)
#set ($region = $core_v2_widget.RegionName.ToLower())
#set ($layout = $core_v2_page.Layout.ToLower())

#if ($region == 'split-sidebar-left' || $region == 'split-sidebar-right' || $region == 'left-sidebar') 
    #set ($showSidebarList = true)
#elseif ($region == 'right-sidebar' && !$layout.Contains('split-content') && !$layout.Contains('split-sidebar'))
    #set ($showSidebarList = true)
#end

#if($showSidebarList)
    $core_v2_widget.ExecuteFile('sidebar.vm')
#else
    $core_v2_ui.PagedContent('standard.vm')
#end
]]></contentScript>
		<headerScript><![CDATA[      
#set($header = $core_v2_widget.GetStringValue('fragmentHeader',''))
#if ($header && $header != "")
    $core_v2_widget.ApplyTokens($header)
#else
    #if ($challenge)
        $challenge.Name
    #else
        $core_v2_language.GetResource('Idea_List_Title')
    #end
#end
]]></headerScript>
		<configuration><![CDATA[      
<propertyGroup id="options" resourceName="Options">
    <property id="fragmentHeader" resourceName="CF_Title" dataType="string" defaultValue="" controlType="Telligent.Evolution.Controls.ContentFragmentTokenStringControl, Telligent.Evolution.Platform" />
    <property id="queryType" resourceName="QueryType" dataType="string" defaultValue="active">
      <propertyValue value="active" resourceName="QueryType_Active" />
      <propertyValue value="all" resourceName="QueryType_All" />
      <propertyValue value="myideas" resourceName="QueryType_My" />
      <propertyValue value="myvotes" resourceName="QueryType_MyVotes" />
    </property>
    <property id="challengeOrGroup" resourceName="CF_ChallengeOrGroup" dataType="string" defaultValue="challenge" controlType="Telligent.Evolution.Controls.PropertyVisibilityValueSelectionControl, Telligent.Evolution.Platform">
        <propertyValue value="challenge" resourceName="CF_ChallengeOrGroup_Challenge" propertiesToShow="selectedChallenge" propertiesToHide="selectedGroup,includeSubGroups" />
        <propertyValue value="group" resourceName="CF_ChallengeOrGroup_Group" propertiesToShow="selectedGroup,includeSubGroups" propertiesToHide="selectedChallenge" />
    </property>    
    <property id="selectedChallenge" resourceName="CF_Idea_SelectChallenge" dataType="custom" controlType="Telligent.Evolution.Extensions.Ideation.ChallengeSelectionControl, Telligent.Evolution.Ideation" width="95%" />
    <property id="selectedGroup" resourceName="CF_Idea_SelectGroup" dataType="custom" controlType="Telligent.Evolution.Controls.GroupSelectionList, Telligent.Evolution.Platform" width="95%" />
	  <property id="includeSubGroups" resourceName="IncludeSubGroups" dataType="bool" defaultValue="false" />
    <property id="excerptSize" resourceName="ExcerptSize" dataType="int" defaultValue="250" descriptionResourceName="ExcerptSize_Description">
		  <propertyRule type="Telligent.Evolution.Controls.PropertyRules.MinMaxValueRule, Telligent.Evolution.Platform" minValue="1" maxValue="10000" />
	  </property>
    <property id="pageSize" resourceName="CF_PageSize" dataType="int" defaultValue="10">
        <propertyRule type="Telligent.Evolution.Controls.PropertyRules.MinMaxValueRule, Telligent.Evolution.Platform" minValue="1" maxValue="100" />
    </property>
</propertyGroup>
      ]]></configuration>
